{
  "expo": {
    "name": "Study Buddy",
    "slug": "study-buddy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "study-buddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-document-picker",
      "expo-image-picker",
      "expo-speech"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}